# Configuration des variables utilisées par docker compose pour n8n
COMPOSE_PROJECT_NAME=n8n
TZ=Europe/Paris
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_EDITOR_BASE_URL=http://localhost:5678
WEBHOOK_URL=http://localhost:5678/
# Génère une clé d'encryption forte (ex: `openssl rand -hex 32`) et décommente :
#N8N_ENCRYPTION_KEY=remplace_par_ta_cle_hex_64
# Auth basique optionnelle pour sécuriser l'UI (décommente si besoin)
#BASIC_AUTH_USER=
#BASIC_AUTH_PASSWORD=
LOCAL_UID=1000
LOCAL_GID=1000
